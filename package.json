{
  "name": "@y2kfund/margin",
  "private": false,
  "version": "0.1.1",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/y2kfund/app-margin.git"
  },
  "homepage": "https://github.com/y2kfund/app-margin#readme",
  "bugs": {
    "url": "https://github.com/y2kfund/app-margin/issues"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "_devNotes": {
    "npm run dev": "uses local package from ../app-core",
    "npm run build": "uses package from Git repository for app-core",
    "dependencies": "The dependencies below are PRODUCTION versions. These are used when npm install is run by cloudflare"
  },
  "scripts": {
    "dev": "npm run deps:dev && vite",
    "build": "npm run deps:prod && vite build",
    "build:lib": "npm run deps:prod && vite build --mode lib",
    "prepublishOnly": "pnpm run build:lib",
    "preview": "vite preview --port 5173",
    "_comment": "deps:dev installs local package temporarily (--no-save), deps:prod ensures Git repo is used",
    "deps:dev": "npm install @y2kfund/core@file:../app-core --no-save",
    "deps:prod": "npm install @y2kfund/core@git+https://github.com/y2kfund/app-core.git --no-save"
  },
  "dependencies": {
    "chart.js": "^4.5.0",
    "vue": "^3.4.0",
    "vue-chartjs": "^5.3.2",
    "vue-router": "^4.3.2"
  },
  "peerDependencies": {
    "@y2kfund/core": "^0.1.0",
    "vue": "^3.4.0"
  },
  "devDependencies": {
    "@tanstack/vue-query": "^5.0.0",
    "@types/node": "^24.4.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "@y2kfund/core": "git+https://github.com/y2kfund/app-core.git",
    "typescript": "^5.4.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^4.5.4",
    "vue-tsc": "^2.0.0"
  }
}
